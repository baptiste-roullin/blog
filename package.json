    "markdown-it-image-figures": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "p-map": "^4.0.0",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.2.2",
    "postcss-hash": "^3.0.0",
    "postcss-import": "^14.0.1",
    "random-seed": "^0.3.0",
    "remove-markdown": "^0.3.0",
    "rimraf": "^3.0.2",
    "simple-nunjucks-loader": "^3.1.0",
