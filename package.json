    "photoswipe": "^5.2.3",
    "sharp": "^0.29.3",
    "text-metrics": "^3.0.0"
  },
  "_moduleAliases": {
    "@src": "./src",
    "@root": "./"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead"
      }
    ]
  ]
}