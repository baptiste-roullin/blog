{
  "name": "tout-ce-qui-bouge",
  "type": "module",
  "scripts": {
    "dev:webpack": "webpack --watch  --mode development",
    "dev:eleventy": "  npx @11ty/eleventy  --incremental --serve --quiet",
    "debug:eleventy": "npx @11ty/eleventy --incremental",
    "dev:postcss": "postcss src/assets/css/main.css -o dist/assets/css/main.css --watch",
    "dev": "npm-run-all --parallel dev:* --print-label",
    "build:webpack": "webpack  --mode production",
    "build:postcss": "cross-env NODE_ENV=production postcss --verbose src/assets/css/main.css -o dist/assets/css/main.css",
    "build:eleventy": "cross-env NODE_ENV=production npx @11ty/eleventy  --quiet --incremental",
    "build": "run-s build:* --print-label"
  },
  "devDependencies": {
    "@11ty/eleventy": "https://github.com/11ty/eleventy",
    "@11ty/eleventy-fetch": "4.0.0",
    "@11ty/eleventy-img": "3.1.6 ",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-rss": "github:baptiste-roullin/eleventy-plugin-rss",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@tailwindcss/forms": "*",
    "@tailwindcss/typography": "*",
    "@types/elasticlunr": "^0.9.5",
    "@types/luxon": "^3.1.0",
    "@types/markdown-it": "^13.0.5",
    "@types/markdown-it-container": "^2.0.5",
    "@types/node": "^20.8.10",
    "@types/nunjucks": "^3.2.1",
    "@types/remove-markdown": "^0.3.1",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "child_process": "^1.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^6.0.0  ",
    "d3-color": "^3.1.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "eleventy-plugin-embed-everything": "^1.14",
    "ffmpeg-static": "^5.1.0",
    "full-icu": "^1.5.0",
    "js-yaml": "^4.1.0",
    "linkedom": "^0.16.1",
    "lodash.clonedeep": "^4.5.0",
    "luxon": "^3.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-blockquote-cite": "github:baptiste-roullin/markdown-it-blockquote-cite",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-highlightjs": "^4.0.1",
    "markdown-it-image-figures": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "p-map": "^6.0.0",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "postcss-hash": "https://github.com/keithamus/postcss-hash",
    "postcss-import": "^15.0.0",
    "random-seed": "^0.3.0",
    "remove-markdown": "^0.5.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "*",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "webpack": "*",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "*",
    "zotero-api-client": "^0.43.1"
  },
  "optionalDependencies": {
    "canvas": "^2.10.2"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-webc": "^0.11.1",
    "elasticlunr": "https://github.com/weixsong/elasticlunr.js",
    "photoswipe": "^5.3.4",
    "text-metrics": "^4.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead"
      }
    ]
  ]
}