    "elasticlunr": "github:weixsong/elasticlunr.js#v0.9.6",
    "photoswipe": "github:dimsemenov/photoswipe#v5-beta",
    "text-metrics": "^2.0.9"
  },
  "_moduleAliases": {
    "@src": "./src",
    "@root": "./"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead"
      }
    ]
  ]
